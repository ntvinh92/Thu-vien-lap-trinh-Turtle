    "import turtle\n",
    "t = turtle.Turtle()\n",
    "\n",
    "# Hình vuông 1\n",
    "for i in range(4):\n",
    "    t.forward(100)\n",
    "    t.left(90)\n",
    "\n",
    "# Hình vuông 2\n",
    "t.penup()\n",
    "t.goto(50, -20.71)\n",
    "t.pendown()\n",
    "t.lt(45)\n",
    "for i in range(4):\n",
    "    t.forward(100)\n",
    "    t.left(90)\n",
    "    \n",
    "turtle.done()"
